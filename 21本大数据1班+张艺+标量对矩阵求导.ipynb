   "source": [
    "import torch\n",
    "\n",
    "\n",
    "# 定义向量a和b\n",
    "a = torch.ones(3, 1)\n",
    "b = torch.ones(3, 1)\n",
    "\n",
    "# 定义3x3的张量X，初始值全为0，并设置requires_grad为True\n",
    "Xb = torch.zeros(3, 3, requires_grad=True)\n",
    "\n",
    "# 计算函数f\n",
    "f = a.t() @ torch.exp(Xb)\n",
    "\n",
    "# 对f进行反向传播，计算梯度\n",
    "f.backward()\n",
    "\n",
    "# 获取X的梯度矩阵\n",
    "gradient = X.grad\n",
    "print(gradient)"
   ]
